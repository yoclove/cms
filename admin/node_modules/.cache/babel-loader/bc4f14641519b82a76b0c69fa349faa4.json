c fetchParents(){\n                const res = await this.$http.get('rest/categories')\n                this.parents = res.data\n            }\n        },\n        watch:{\n            '$route.params.id': function (id) {\n                this.model = {};\n\n            }\n\n        },\n        created(){\n            this.id && this.fetch()\n            this.fetchParents()\n        }\n    }\n</script>\n"],"sourceRoot":"src/views"}]}